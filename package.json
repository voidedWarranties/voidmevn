{
  "name": "voidmevn",
  "version": "1.0.0",
  "description": "My version of Mongo Express Vue Node",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"babel-node scripts/start.js\" \"nodemon --exec babel-node -- src/server/index.js\"",
    "build": "babel-node scripts/build.js && babel src/server -d dist/server --copy-files",
    "serve": "cross-env NODE_ENV=production node dist/server/index.js",
    "clean": "shx rm -rf dist"
  },
