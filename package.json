  "dependencies": {
    "@babel/cli": "^7.4.4",
    "axios": "^0.18.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.19.0",
    "connect-flash": "^0.1.1",
    "connect-history-api-fallback": "^1.6.0",
    "connect-mongo": "^2.0.3",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "express-static-gzip": "^1.1.3",
    "mongoose": "^5.5.9",
